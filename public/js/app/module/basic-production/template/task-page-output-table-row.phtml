<script id="task-page-output-table-row-template" type="text/template">
    <td>
        <i class="fas fa-dolly fa-fw"></i>
    </td>
    <td>
        <div>
            <%- article.name %>
        </div>
        <div class="description">
            <% if (variety) { %>
            <span><%- plant.name %> <%- variety.name %></span>
            <% } else { %>
            <span><%- polyglot.t('model.field.variety_id.null').toLowerCase() %></span>
            <% } %>
        </div>
    </td>
    <td>
        <div class="right"><%- quantity %> <%- quantity_unit.value %></div>
    </td>
</script>